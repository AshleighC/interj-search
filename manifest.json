{
  "manifest_version": 2,

  "name": "Interjective Search",
  "description": "Search for worrrrrrds.",
  "version": "1.0",

  "permissions": [
    "https://www.google.com/search?*",
    "tabs"
  ],

  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ]
  },

  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "browser_action": {
    "default_title": "Interjective Search",
    "default_popup": "popup.html"
  }
}
